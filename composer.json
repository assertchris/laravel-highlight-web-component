{
    "name": "tempest/w-code",
    "description": "A Laravel plugin with a custom Web Component for highlighting code, using tempest/highlight.",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Pitt",
            "email": "me@assertchris.io",
            "homepage": "https://assertchris.io",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/assertchris/w-code",
    "require": {
        "php": "^8.3",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "tempest/highlight": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tempest\\Wcode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Wcode\\Tests\\": "tests",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tempest\\Wcode\\WcodeProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    }
}
